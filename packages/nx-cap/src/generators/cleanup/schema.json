{
  "$schema": "https://json-schema.org/schema",
  "$id": "Cleanup",
  "title": "Cleanup boostrapped project",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Project name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the project name?"
    }
  },
  "required": [
    "name"
  ]
}